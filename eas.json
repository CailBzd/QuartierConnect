{      
  "cli": {
    "version": ">=12.5.1"
  },  
  "build": {
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.production.com",
        "APP_ICON": "./assets/icon-test.png",
        "APP_NAME": "Vivilink",        
        "VERSION_CODE": "4"
      },
      "android": {
        "buildType": "app-bundle",        
        "autoIncrement": "versionCode" 
      }
    },
    "test": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.test.com",
        "APP_ICON": "./assets/icon-test.png",
        "APP_NAME": "Vivilink"
      },
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": "versionCode" 
        }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./assets/vivilink-prod.json",
        "track": "alpha"
      }
    },
    "test": {
      "android": {
        "serviceAccountKeyPath": "./assets/vivilink-prod.json",
        "track": "internal"
      }
    }
  }
}
  
